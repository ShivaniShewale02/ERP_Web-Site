<%- include("../partials/header"); -%>
<div class="freeSlot">
<% var mondayTime = [] %>
<% var tuesdayTime = [] %>
<% var wednesdayTime = [] %>
<% var thursdayTime = [] %>
<% var fridayTime = [] %>
<% var saturdayTime = [] %>

  <div style="transform: translateY(-20px);" class="w3-container">
    <h3 style="text-align: center; margin-bottom: 20px;">Free Slot of the week</h3>
  <table class="w3-table-all w3-centered w3-card-4">
    <tr>
      <th>Sr No.</th>
      <th>Day</th>
      <th>Free Slot Time</th>
      <th>No. of free slot</th>
    </tr>
    <tr>
      <td>1</td>
      <td>Monday</td>
      <td class="facFreeSlot">
        <% monday.forEach((obj)=>{ %>
        <% if(obj.subject === "FREE"){ %>
           <span><%=obj.time%></span> <br>
            <%mondayTime.push(obj.time) %>
        <%  } %>
        <% }) %>

      </td>
      <td><%=mondayTime.length%></td>
    </tr>
    <tr>
      <td>2</td>
      <td>Tuesday</td>
      <td class="facFreeSlot">
        <% tuesday.forEach((obj)=>{ %>
        <% if(obj.subject === "FREE"){ %>
           <span><%=obj.time%></span> <br>
            <%tuesdayTime.push(obj.time) %>
        <%  } %>
        <% }) %>

      </td>
      <td><%=tuesdayTime.length%></td>
    </tr>
    <tr>
      <td>3</td>
      <td>Wednesday</td>
      <td class="facFreeSlot">
        <% wednesday.forEach((obj)=>{ %>
        <% if(obj.subject === "FREE"){ %>
           <span><%=obj.time%></span> <br>
            <%wednesdayTime.push(obj.time) %>
        <%  } %>
        <% }) %>

      </td>
      <td><%=wednesdayTime.length%></td>
    </tr>
    <tr>
      <td>4</td>
      <td>Thursday</td>
      <td class="facFreeSlot">
        <% thursday.forEach((obj)=>{ %>
        <% if(obj.subject === "FREE"){ %>
           <span><%=obj.time%></span> <br>
            <%thursdayTime.push(obj.time) %>
        <%  } %>
        <% }) %>

      </td>
      <td><%=thursdayTime.length%></td>
    </tr>
    <tr>
      <td>5</td>
      <td>Friday</td>
      <td class="facFreeSlot">
        <% friday.forEach((obj)=>{ %>
        <% if(obj.subject === "FREE"){ %>
           <span><%=obj.time%></span> <br>
            <%fridayTime.push(obj.time) %>
        <%  } %>
        <% }) %>

      </td>
      <td><%=fridayTime.length%></td>
    </tr>
    <tr>
      <td>6</td>
      <td>Saturday</td>
      <td class="facFreeSlot">
        <% saturday.forEach((obj)=>{ %>
        <% if(obj.subject === "FREE"){ %>
           <span><%=obj.time%></span> <br>
            <%saturdayTime.push(obj.time) %>
        <%  } %>
        <% }) %>

      </td>
      <td><%=saturdayTime.length%></td>
    </tr>
  </table>
  </div>
</div>
<%- include("../partials/footer"); -%>
